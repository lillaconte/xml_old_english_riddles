         <lg style="poem">
            <l met="verse">
               Ic eom <seg type="alliteration" subtype="m">m</seg>are      
               <milestone unit="caesura"/>þonne þes <seg type="alliteration" subtype="m">m</seg>iddangeard
            </l>
            <l met="verse"> <seg type="alliteration" subtype="l">l</seg>æsse þonne hondwyrm,      
               <milestone unit="caesura"/><seg type="alliteration" subtype="l">l</seg>eohtre þonne <eg style="sky_lexical_field">mona</eg>,
            </l>
            <l met="verse"> <seg type="alliteration" subtype="s">s</seg>wiftre þonne <eg style="sky_lexical_field"><seg type="alliteration" subtype="s">s</seg>unne</eg>.      
               <milestone unit="caesura"/><seg type="alliteration" subtype="s">S</seg>æs me <seg type="alliteration" subtype="s">s</seg>ind ealle
            </l>
            <l met="verse"> <seg type="alliteration" subtype="f">f</seg>lodas on <seg type="alliteration" subtype="f">f</seg>æðmum      
               <milestone unit="caesura"/>ond þes <seg type="alliteration" subtype="f">f</seg>oldan bearm,
            </l>
            <l met="verse"> <seg type="alliteration" subtype="g">g</seg>rene wongas.      
               <milestone unit="caesura"/><seg type="alliteration" subtype="g">G</seg>rundum ic hrine,
            </l>
            <l met="verse"> <seg type="alliteration" subtype="h">h</seg>elle under<seg type="alliteration" subtype="h">h</seg>nige,      
               <milestone unit="caesura"/><eg style="sky_lexical_field"><seg type="alliteration" subtype="h">h</seg>eofonas</eg> oferstige,
            </l>
            <l met="verse"> <seg type="alliteration" subtype="w">w</seg>uldres eþel,      
               <milestone unit="caesura"/><seg type="alliteration" subtype="w">w</seg>ide ræce
            </l>
            <l met="verse">ofer <eg style="sky_lexical_field"><seg type="alliteration" subtype="e">e</seg>ngla <seg type="alliteration" subtype="e">e</seg>ard</eg>,      
               <milestone unit="caesura"/><seg type="alliteration" subtype="e">e</seg>orþan gefylle,
            </l>
            <l met="verse">ealne <seg type="alliteration" subtype="m">m</seg>iddangeard      
               <milestone unit="caesura"/>ond <seg type="alliteration" subtype="m">m</seg>erestreamas
            </l>
            <l met="verse" ana="end_formula"> <seg type="alliteration" subtype="s">s</seg>ide mid me <seg type="alliteration" subtype="s">s</seg>ylfum.      
               <milestone unit="caesura"/><seg type="alliteration" subtype="s">S</seg>aga hwæt ic hatte.
            </l>
